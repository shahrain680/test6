<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/six">


    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="416dp"
        android:text="TextView" />

    <Button
        android:id="@+id/AIButton"
        android:layout_width="205dp"
        android:layout_height="65dp"
        android:layout_gravity="center"
        android:background="#00FF9800"
        android:fontFamily="@font/aovelsansrounded"
        android:gravity="center"
        android:onClick="aiClick"
        android:text="Scan Item"
        android:textColor="#2C2A2A"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:text="TextView"
        android:visibility="invisible" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:fontFamily="@font/nevisbold"
        android:text="Log In" />

</LinearLayout>
